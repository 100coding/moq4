{
	"version": "1.0.0-*",
	"description": "Moq.NetCore.UnitTests Class Library",
	"authors": [ "jeremymeng" ],
	"tags": [ "" ],
	"projectUrl": "",
	"licenseUrl": "",

	"compile": [
		"../../UnitTests/**/*.cs"
