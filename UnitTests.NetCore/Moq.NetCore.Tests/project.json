        "Moq": {
          "target": "project",
          "version": "99.99.99-wrapped"
        },
        "NETStandard.Library": "1.5.0-rc2-23928",
        "System.Collections.Specialized": "4.0.1-rc2-23928",
        "System.ComponentModel": "4.0.1-rc2-23928",
        "System.ComponentModel.Primitives": "4.0.1-rc2-23928",
        "System.Linq": "4.1.0-rc2-23928",
        "System.Linq.Queryable": "4.0.1-rc2-23928",
        "System.ServiceModel.Primitives": "4.1.0-rc2-23928",
        "System.Threading": "4.0.11-rc2-23928",
